{
  "name": "kijiji-monitoring",
  "private": true,
  "homepage": "/kijiji-monitoring/#",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tsc": "tsc",
    "predeploy": "npm run build",
    "deploy": "tsc && vitest run && vite build && gh-pages -d dist",
    "build": "tsc && vitest run && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tww": "npx tailwindcss -i ./src/assets/css/main.css -o ./dist.dev/output.css --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "html-entities": "^2.4.0",
    "jsdom": "^22.1.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "sort-by": "^0.0.2",
    "timeago-react": "^3.0.6",
    "uniqid": "^5.4.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uniqid": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.34.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.34.3"
  }
}
